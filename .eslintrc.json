{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/no-absolute-path": "off",
    "linebreak-style": 0,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]
  }
}
